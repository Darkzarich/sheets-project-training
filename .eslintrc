{
  "root": true,
  "parser": "@babel/eslint-parser",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": ["eslint:recommended", "google", "plugin:prettier/recommended"],
  "rules": {
    "semi": ["warn", "never"],
    "require-jsdoc": "off",
    "no-else-return": ["warn"]
  }
}
